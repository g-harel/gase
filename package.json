{
  "name": "goo",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "publish": "npm publish ./goo-js & npm publish ./goo-dom & npm publish ./goo-state & npm publish ./goo-utils & npm publish ./goo-router & npm publish ./goo-history & del npm-debug.log",
    "test": "mocha \"*/*test.js\"",
    "test:watch": "nodemon -x \"npm run test -s\"",
    "coverage": "istanbul cover node_modules/mocha/bin/_mocha */*test.js -x */*test.js",
    "coverage:open": "npm run coverage & start coverage/lcov-report/index.html",
    "coverage:coveralls": "npm run coverage & cat coverage/lcov.info | coveralls",
    "bundle": "webpack",
    "bundle:watch": "webpack --watch",
    "bunlde:minify": "webpack -p"
  },
  "homepage": "http://github.com/g-harel/goo",
  "repository": {
    "type": "git",
    "url": "http://github.com/g-harel/goo"
  },
  "devDependencies": {
    "babel-core": "^6.22.1",
    "babel-loader": "^6.2.10",
    "babel-preset-es2015": "^6.22.0",
    "chai": "^3.5.0",
    "compression-webpack-plugin": "^0.4.0",
    "coveralls": "^2.13.1",
    "eslint": "^3.15.0",
    "eslint-config-google": "^0.7.1",
    "istanbul": "^0.4.5",
    "jsdom": "^9.12.0",
    "mocha": "^3.4.2",
    "nodemon": "^1.11.0",
    "webpack": "^2.2.1"
  },
  "dependencies": {
    "goo-js": "^2.3.0"
  }
}
