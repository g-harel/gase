{
  "name": "goo",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "publish:goo-js": "npm publish ./packages/goo-js",
    "publish:goo-dom": "npm publish ./packages/goo-dom",
    "publish:goo-state": "npm publish ./packages/goo-state",
    "publish:goo-utils": "npm publish ./packages/goo-utils",
    "publish:goo-router": "npm publish ./packages/goo-router",
    "publish:goo-history": "npm publish ./packages/goo-history",
    "publish:clean": "del npm-debug.log",
    "publish": "run-s publish:*",
    "test": "run-s test:eslint test:mocha",
    "test:mocha": "mocha ./packages/**/test.js",
    "test:eslint": "eslint ./packages/**/*.js",
    "test:watch": "nodemon -x \"npm run test:mocha -s\"",
    "coverage": "istanbul cover node_modules/mocha/bin/_mocha ./packages/**/test.js -x ./packages/**/test.js",
    "coverage:open": "run-s coverage showcov",
    "coverage:coveralls": "run-s coverage coveralls",
    "showcov": "start coverage/lcov-report/index.html",
    "coveralls": "cat coverage/lcov.info | coveralls",
    "bundle": "webpack",
    "bundle:watch": "webpack --watch",
    "bundle:minify": "webpack -p"
  },
  "repository": {
    "type": "git",
    "url": "http://github.com/g-harel/goo"
  },
  "devDependencies": {
    "babel-core": "^6.22.1",
    "babel-loader": "^6.2.10",
    "babel-preset-es2015": "^6.22.0",
    "chai": "^3.5.0",
    "compression-webpack-plugin": "^0.4.0",
    "coveralls": "^2.13.1",
    "emn": "^0.1.2",
    "eslint": "^3.15.0",
    "eslint-config-google": "^0.7.1",
    "istanbul": "^0.4.5",
    "jsdom": "^9.12.0",
    "mocha": "^3.4.2",
    "nodemon": "^1.11.0",
    "npm-run-all": "^4.0.2",
    "webpack": "^2.2.1"
  },
  "dependencies": {
    "path-to-regexp": "^1.7.0"
  }
}
